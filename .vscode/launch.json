{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Train Model",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/trainval.py",
            "console": "integratedTerminal",
            "args": [
                "--dataset", "ApolloScale",
                "--SDD_if_filter", "True",
                "--test_set", "apollo",
                "--neighbor_thred", "10",
                "--device", "1",
                "--phase", "train",
                "--stage", "MLDG",
                "--train_model", "Dual_TT",
                "--batch_around_ped", "256",
                "--batch_around_ped_meta", "256",
                "--start_test", "0",
                "--PE", "False",
                "--num_epochs", "1000",
                "--Dual_TT_ablation", "Dual_TT",
                "--param_diff", "Apollo_test_qianyi",
                "--seq_length", "12" ,
                "--obs_length", "6" ,
                "--pred_length", "6",
            ],
            "env": {
                "PYTHONUNBUFFERED": "1"
            }
        }
    ]
}
